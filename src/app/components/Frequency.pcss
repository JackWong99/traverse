#frequency-list {
  display: flex;
  justify-content: space-around;
  list-style: none;
  margin: 0;
  padding: 0;
  border-bottom: 1px solid var(--grey);

  & li {
    flex: 1;
    text-align: center;
  }

  & li a {
    display: block;
    padding: 0.5rem;
    background-color: var(--black);

    &:hover,
    &.selected {
      background-color: var(--white);
    }
  }
}
